<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" styleClass="root" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uboat.screens.main.MainScreen">
    <content>
        <VBox alignment="TOP_CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
            <children>
                <fx:include fx:id="fileManager" source="file/manager/FileManager.fxml" />
                <Separator prefWidth="200.0" />
                <fx:include fx:id="tabsManager" source="tabs/manager/TabsManager.fxml" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
    </content>
</ScrollPane>
