<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.tabs.brute.force.panels.decryption.input.encryption.BruteForceEncryptionPanel">
   <children>
      <HBox minHeight="-Infinity" spacing="10.0">
         <children>
            <Label minHeight="-Infinity" minWidth="-Infinity" prefWidth="146.0" text="Message to encrypt:">
               <font>
                  <Font name="Berlin Sans FB" size="18.0" />
               </font>
               <padding>
                  <Insets top="2.0" />
               </padding>
            </Label>
            <TextField fx:id="selectedMessageTextField" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="300.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox minHeight="-Infinity" spacing="10.0">
         <children>
            <Button fx:id="resetToInitialConfigurationButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Reset To Initial Configuration" />
            <Button alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#clearTextFields" text="Clear Text Field" />
         </children>
      </HBox>
      <fx:include fx:id="dictionaryPanel" source="dictionary/DictionaryPanel.fxml" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
